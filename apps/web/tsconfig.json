{
    "extends": "@altas/tsconfig/nextjs.json",
    "compilerOptions": {
        "plugins": [
            {
                "name": "next"
            }
        ],
        "module": "esnext",

        "tsBuildInfoFile": "./node_modules/.cache/tsbuildinfo.json"
    },
    "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
    "exclude": ["node_modules"]
}
